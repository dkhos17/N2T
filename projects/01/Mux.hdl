/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand(a=sel, b=sel, out=notsel);
    And(a=a, b=notsel, out=outa);
    And(a=sel, b=b, out=outb);
    Or(a=outa, b=outb, out=out);
}